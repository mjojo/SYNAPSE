// Test: just one char with setbyte in func
fn str_len(s) {
    let i = 0
    while getbyte(s, i) > 0 {
        i = i + 1
    }
    return i
}

fn io_print(s) {
    let len = str_len(s)
    let h = getstd(-11)
    write(h, s, len)
    return 0
}

fn print_first(source) {
    let buf = alloc(2)
    setbyte(buf, 1, 0)
    
    let c = getbyte(source, 0)
    setbyte(buf, 0, c)
    io_print(buf)
    return 0
}

fn main() {
    let code = alloc(8)
    setbyte(code, 0, 65)   // A
    setbyte(code, 1, 66)   // B
    setbyte(code, 2, 0)
    
    print_first(code)
    
    return 0
}
