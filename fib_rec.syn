// fib_rec.syn - RECURSIVE FIBONACCI
// Phase 17: The Ultimate Test!
// O(2^n) complexity - tests stack frames!

fn fib(n) {
    if (n < 2) {
        return n
    }
    return fib(n - 1) + fib(n - 2)
}

fn main() {
    print(fib(0))       // Should be 0
    print(fib(1))       // Should be 1
    print(fib(5))       // Should be 5
    print(fib(10))      // Should be 55
    print(fib(15))      // Should be 610
    
    return 0
}
