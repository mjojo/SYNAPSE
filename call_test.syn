// call_test.syn - Test function call in expression

fn echo(x) {
    print(x)
    return x
}

fn main() {
    print(777)          // Marker
    
    let a = echo(5)     // Should print 5, a = 5
    print(a)            // Should print 5
    
    let b = echo(a - 1) // Should print 4, b = 4
    print(b)            // Should print 4
    
    return 0
}
