// Test is_alpha with Z
fn is_alpha(c) {
    let result = 0
    if c >= 97 {
        if c <= 122 { 
            result = 1 
        }
    }
    if result == 1 { return 1 }
    
    if c >= 65 {
        if c <= 90 { 
            result = 1 
        }
    }
    if result == 1 { return 1 }
    
    if c == 95 { return 1 }
    
    return 0
}

fn main() {
    let r = is_alpha(90)  // 'Z'
    return r
}
