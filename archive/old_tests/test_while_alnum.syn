// Debug while loop with is_alnum
fn is_alpha(c) {
    if c >= 97 {
        if c <= 122 { return 1 }
    }
    if c >= 65 {
        if c <= 90 { return 1 }
    }
    if c == 95 { return 1 }
    return 0
}

fn is_digit(c) {
    if c < 48 { return 0 }
    if c > 57 { return 0 }
    return 1
}

fn is_alnum(c) {
    if is_alpha(c) == 1 { return 1 }
    if is_digit(c) == 1 { return 1 }
    return 0
}

fn main() {
    let code = alloc(8)
    setbyte(code, 0, 102)  // f
    setbyte(code, 1, 110)  // n
    setbyte(code, 2, 32)   // space
    setbyte(code, 3, 0)
    
    let len = 0
    let j = 0
    let ch = getbyte(code, j)
    
    while is_alnum(ch) == 1 {
        len = len + 1
        j = j + 1
        ch = getbyte(code, j)
    }
    
    // Should be 2 (f, n)
    return len
}
