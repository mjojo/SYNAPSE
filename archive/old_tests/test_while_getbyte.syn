// Test getbyte in while condition - simplified, no function call
fn main() {
    let buf = alloc(10)
    setbyte(buf, 0, 65)  // A
    setbyte(buf, 1, 66)  // B
    setbyte(buf, 2, 0)   // null terminator
    
    let i = 0
    let ch = getbyte(buf, i)
    
    while ch > 0 {
        i = i + 1
        ch = getbyte(buf, i)
    }
    return i
}
