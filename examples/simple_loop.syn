fn main() {
    let filename = "examples/small.syn"
    let handle = fopen(filename, 0)
    
    if (handle == 0) {
        puts("Error open")
        return 1
    }
    
    let src = alloc(4096)
    let len = fread(handle, src, 4096)
    fclose(handle)
    
    print(len)
    
    let i = 0
    let c = 0
    
    while (i < len) {
        c = get_byte(src, i)
        print(c)
        i = i + 1
    }
    
    puts("Done")
    return 0
}
