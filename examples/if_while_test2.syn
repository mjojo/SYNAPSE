// Simple if-while test - vars outside if
fn main() {
    print(1111)
    
    let ok = 1
    let x = 0
    
    if (ok) {
        print(2222)
        
        x = 0
        while (x < 3) {
            print(x)
            x = x + 1
        }
        
        print(3333)
    }
    
    print(4444)
    return 0
}
