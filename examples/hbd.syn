// Happy Birthday Script for The Creator
// Uses: Stack args, Heap alloc, Loops, Arithmetic

fn make_cake(layers) {
    let i = 0
    while (i < layers) {
        print(888888)
        let i = i + 1
    }
    print(1)
    return 0
}

fn main() {
    print(2312)
    
    let gifts = alloc(3)
    gifts[0] = 2025
    gifts[1] = 100
    gifts[2] = 777
    
    make_cake(3)
    
    print(999999)
    
    print(gifts[0])
    print(gifts[1])
    print(gifts[2])
    
    return 0
}
