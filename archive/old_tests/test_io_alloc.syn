// Test io_print on alloc buffer
fn str_len(s) {
    let i = 0
    while getbyte(s, i) > 0 {
        i = i + 1
    }
    return i
}

fn io_print(s) {
    let len = str_len(s)
    let h = getstd(-11)
    write(h, s, len)
    return 0
}

fn main() {
    let buf = alloc(4)
    setbyte(buf, 0, 65)  // A
    setbyte(buf, 1, 66)  // B
    setbyte(buf, 2, 0)
    
    io_print(buf)
    
    return 0
}
