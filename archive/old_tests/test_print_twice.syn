// Test calling my_print twice
fn str_len(s) {
    let i = 0
    while getbyte(s, i) > 0 {
        i = i + 1
    }
    return i
}

fn my_print(s) {
    let len = str_len(s)
    let h = getstd(-11)
    write(h, s, len)
    return 0
}

fn main() {
    let a = alloc(3)
    setbyte(a, 0, 65)   // A
    setbyte(a, 1, 0)
    
    let b = alloc(3)
    setbyte(b, 0, 66)   // B
    setbyte(b, 1, 0)
    
    my_print(a)
    my_print(b)
    return 42
}
