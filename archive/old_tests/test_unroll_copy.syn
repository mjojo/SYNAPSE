// Unrolled copy - no while
fn str_len(s) {
    let i = 0
    while getbyte(s, i) > 0 {
        i = i + 1
    }
    return i
}

fn io_print(s) {
    let len = str_len(s)
    let h = getstd(-11)
    write(h, s, len)
    return 0
}

fn main() {
    let code = alloc(4)
    setbyte(code, 0, 65)  // A
    setbyte(code, 1, 66)  // B
    setbyte(code, 2, 0)
    
    let buf = alloc(4)
    
    // Unrolled copy
    let ch0 = getbyte(code, 0)
    setbyte(buf, 0, ch0)
    
    let ch1 = getbyte(code, 1)
    setbyte(buf, 1, ch1)
    
    setbyte(buf, 2, 0)
    
    io_print(buf)
    
    return 0
}
