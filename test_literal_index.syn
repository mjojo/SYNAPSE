// Test with literal index
fn main() {
    let arr = alloc(2000)
    
    // Use literal 0 as index (not variable)
    arr[0] = 11
    arr[10] = 22
    arr[100] = 33
    
    // Read back using literals
    let v0 = arr[0]
    let v10 = arr[10]
    let v100 = arr[100]
    
    let sum = v0 + v10 + v100
    
    puts("Sum of arr[0,10,100] (expect 11+22+33=66):")
    print(sum)
    
    return 0
}
