// Test calling print2 twice
fn str_len(s) {
    let i = 0
    while getbyte(s, i) > 0 {
        i = i + 1
    }
    return i
}

fn print2(s) {
    let len = str_len(s)
    let h = getstd(-11)
    write(h, s, len)
    return len
}

fn main() {
    let a = alloc(4)
    setbyte(a, 0, 65)  // A
    setbyte(a, 1, 0)
    
    let b = alloc(4)
    setbyte(b, 0, 66)  // B  
    setbyte(b, 1, 0)
    
    print2(a)
    print2(b)
    return 42
}
