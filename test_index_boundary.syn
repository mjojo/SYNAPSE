// Test large index boundary
fn main() {
    let arr = alloc(2000)
    
    // Test various indices
    let i0 = 0
    arr[i0] = 11
    
    let i10 = 10
    arr[i10] = 22
    
    let i100 = 100
    arr[i100] = 33
    
    let i500 = 500
    arr[i500] = 44
    
    // Read back
    let v0 = arr[i0]
    let v10 = arr[i10]
    let v100 = arr[i100]
    let v500 = arr[i500]
    
    // We can only print once after puts, so sum them
    let sum = v0 + v10 + v100 + v500
    
    puts("Sum of arr[0,10,100,500] (expect 11+22+33+44=110):")
    print(sum)
    
    return 0
}
