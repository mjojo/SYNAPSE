// debug_cli.syn - Debug CLI parsing

fn str_len(s) {
    let i = 0
    while getbyte(s, i) > 0 {
        i = i + 1
    }
    return i
}

fn io_print(s) {
    let len = str_len(s)
    let h = getstd(-11)
    write(h, s, len)
    return 0
}

fn main() {
    let cmd = get_cmd_line()
    let h = getstd(-11)
    write(h, cmd, 100)
    return 0
}
