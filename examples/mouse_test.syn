fn main() {
    window(800, 600)
    let running = 1
    let counter = 0
    while running {
        let mx = mouse_x()
        let my = mouse_y()
        clear_screen(0x0000FF)
        update_window()
        sleep(16)
        counter = counter + 1
        if counter == 100 {
            running = 0
        }
    }
    print(counter)
    return 0
}
