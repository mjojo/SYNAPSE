// Test print function
fn str_len(s) {
    let i = 0
    while getbyte(s, i) > 0 {
        i = i + 1
    }
    return i
}

fn print(s) {
    let len = str_len(s)
    let h = getstd(-11)
    write(h, s, len)
    return 0
}

fn main() {
    let msg = alloc(5)
    setbyte(msg, 0, 72)   // H
    setbyte(msg, 1, 105)  // i
    setbyte(msg, 2, 33)   // !
    setbyte(msg, 3, 0)    // null
    
    print(msg)
    return 42
}
