// Test nested if inside while - minimal
fn main() {
    let s = "42"
    let pos = 0
    let len = 2
    
    while pos < len {
        let c = get_byte(s, pos)
        
        print(1001)
        print(c)
        
        let is_digit = 0
        if c > 47 {
            print(2001)
            if c < 58 {
                print(2002)
                is_digit = 1
            }
        }
        
        print(3001)
        print(is_digit)
        
        pos = pos + 1
    }
    
    return 0
}
