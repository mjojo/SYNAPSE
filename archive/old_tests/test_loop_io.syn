// Simplest loop with io_print
fn str_len(s) {
    let i = 0
    while getbyte(s, i) > 0 {
        i = i + 1
    }
    return i
}

fn io_print(s) {
    let len = str_len(s)
    let h = getstd(-11)
    write(h, s, len)
    return 0
}

fn main() {
    let msg = alloc(3)
    setbyte(msg, 0, 88)  // X
    setbyte(msg, 1, 0)
    
    let i = 0
    while i < 3 {
        io_print(msg)
        i = i + 1
    }
    
    return 0
}
