// Debug copy_ident step by step
fn str_len(s) {
    let i = 0
    while getbyte(s, i) > 0 {
        i = i + 1
    }
    return i
}

fn io_print(s) {
    let len = str_len(s)
    let h = getstd(-11)
    write(h, s, len)
    return 0
}

fn is_alpha(c) {
    if c >= 97 {
        if c <= 122 { return 1 }
    }
    if c >= 65 {
        if c <= 90 { return 1 }
    }
    if c == 95 { return 1 }
    return 0
}

fn is_digit(c) {
    if c < 48 { return 0 }
    if c > 57 { return 0 }
    return 1
}

fn is_alnum(c) {
    if is_alpha(c) == 1 { return 1 }
    if is_digit(c) == 1 { return 1 }
    return 0
}

fn main() {
    // "fn x"
    let code = alloc(8)
    setbyte(code, 0, 102)  // f
    setbyte(code, 1, 110)  // n
    setbyte(code, 2, 32)   // space
    setbyte(code, 3, 120)  // x
    setbyte(code, 4, 0)
    
    // Read first char
    let j = 0
    let ch = getbyte(code, j)
    
    // Check is_alnum
    let is_an = is_alnum(ch)
    
    // Return: ch * 1000 + is_an
    // Expected: 102000 + 1 = 102001
    return is_an
}
